/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background:#FAF8F5 100%;
} 
.tag-symbol{
  display: inline-block;
  padding:3rpx 16rpx;
  border:1px solid #E6AC00;
  font-size: 24rpx;
  color: #E6AC00;
  letter-spacing: -0.83rpx;
  margin-right:10rpx;
  border-radius:2rpx;
}
